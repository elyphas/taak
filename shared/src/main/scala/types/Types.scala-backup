sealed trait Father {
    val id: String
    val descripcion: Option[String]
}

case class TypeDocument(
   id: String,
   descripcion: Option[String],
   observation: Option[String]
) extends Father

object TypeDocument {
    def apply = new TypeDocument(id = "", descripcion = None, observation = None)
}

case class Programa(
